{
    "plugins": [ "edit-link", "theme-api", "jsfiddle", "custom-favicon" ],
    "pluginsConfig": {
        "edit-link": {
            "base": "https://github.com/i18next/i18next-gitbook/edit/master/",
            "label": "Edit This Page"
        },
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "js",
                    "default": true
                }
            ]
        },
        "jsfiddle":{
            "type": "script",
            "tabs": ["result","js"],
            "height": "500",
            "width": "500"
        },
        "favicon": "assets/favicon.ico"
    },
    "links": {
        "home": true,
        "about": true,
        "issues": true,
        "contribute": true,
        "sidebar": {
            "Custom link name": "https://customlink.com"
        },
        "sharing": {
            "google": true,
            "facebook": true,
            "twitter": true,
            "github": true
        }
    }
}
