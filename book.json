{
    "plugins": [ "theme-api", "jsfiddle", "custom-favicon" ],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "jsfiddle":{
            "type": "script",
            "tabs": ["result","js"],
            "height": "500",
            "width": "500",
            "fontColor": "00FF00"
        },
        "favicon": "assets/favicon.ico"
    }
}
