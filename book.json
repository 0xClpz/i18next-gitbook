{
    "plugins": [ "theme-api", "jsfiddle", "custom-favicon", "anchorjs" ],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "go",
                    "name": "Go"
                }
            ]
        },
        "jsfiddle":{
            "type": "script",
            "tabs": ["result","js"],
            "height": "500",
            "width": "500"
        },
        "favicon": "assets/favicon.ico",
        "anchorjs": {
          "icon": "#",
          "placement": "left",
          "visible": "always",
          "class": "custom-class another-one"
        }    
    }
}
